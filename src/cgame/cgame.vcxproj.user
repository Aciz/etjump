<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
    <DebuggerFlavor>WindowsRemoteDebugger</DebuggerFlavor>
    <RemoteDebuggerDebuggerType>NativeOnly</RemoteDebuggerDebuggerType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <LocalDebuggerCommand>D:\Modding\ET\et.exe</LocalDebuggerCommand>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerDebuggerType>NativeOnly</LocalDebuggerDebuggerType>
    <LocalDebuggerAttach>false</LocalDebuggerAttach>
    <LocalDebuggerCommandArguments>+connect localhost:35000</LocalDebuggerCommandArguments>
    <LocalDebuggerWorkingDirectory>D:\Modding\ET\</LocalDebuggerWorkingDirectory>
  </PropertyGroup>
</Project>